<?xml version="1.0" ?>
<robot name="za6" xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:arg name="prefix" default=""/>
  <xacro:include
      filename="$(find za6_tools)/urdf/versabuilt_multigrip_macro.xacro"/>

  <!-- 'flange' frame: default pose reference frame -->
  <link name="flange" />

  <xacro:versabuilt_multigrip_macro
      prefix="$(arg prefix)"
      connected_to="flange"/>
</robot>
